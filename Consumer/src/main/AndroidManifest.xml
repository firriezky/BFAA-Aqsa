<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aqsa.consumer">

    <uses-permission android:name="com.aqsa.myapplication.READ_DATABASE" />
    <uses-permission android:name="com.aqsa.myapplication.WRITE_DATABASE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <permission
        android:name="com.aqsa.myapplication.READ_DATABASE"
        android:protectionLevel="normal" />
    <permission
        android:name="com.aqsa.myapplication.READ_DATABASE"
        android:protectionLevel="normal" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_code"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_code"
        android:supportsRtl="true"
        android:theme="@style/Theme.LabgitAqsa">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <queries>
        <package android:name="com.aqsa.myapplication" />
    </queries>

</manifest>